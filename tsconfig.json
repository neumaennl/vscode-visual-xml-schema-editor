{
	"compilerOptions": {
        "experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"module": "CommonJS",
		"target": "ES6",
		"outDir": "out",
		"sourceMap": false,
		"inlineSourceMap": true,
		"inlineSources": true,
		"strict": true,
		"rootDir": "src",
		"esModuleInterop": true,
        "noUnusedLocals": true,
        "noImplicitOverride": true,
        "skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
        "strictPropertyInitialization": false,
	},
	"include": [
	  "src/**/*.ts"
	],
	
}
